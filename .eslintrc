{
"extends": [ "airbnb","plugin:react/recommended"],
  "plugins": [ "react" ,"import"],
  "env": {
    "browser" :true,
    "es6": true,
    },
  "parser": "babel-eslint",
  "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "rules": {
        "semi": 2,
        "import/named": 2,
        "react/prop-types": 1,
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
        //"prettier/prettier": "1"
    }
}
